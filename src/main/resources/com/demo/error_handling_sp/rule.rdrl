package com.demo.error_handling_sp;

import java.lang.Number;

rule "rule"
	dialect "mvel"
	ruleflow-group "ruleflow1"
	when
		p : Post( id == 1 )
	then
		p.setTitle( "Title generated by the rule 1" );
		System.out.println(">>> [Rule 1] Setting title of Post[ id= 1] to : Title generated by the rule 1" );
end
