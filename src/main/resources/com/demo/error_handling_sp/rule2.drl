package com.demo.error_handling_sp;

import java.lang.Number;

rule "rule2"
	dialect "mvel"
	ruleflow-group "ruleflow2"
	when
		p : Post( id == 1 )
	then
		p.setTitle("Title generated by the rule 2" );
		System.out.println(">>> [Rule 2] Setting title of Post[ id= 1] to : Title generated by the rule 2" );
end
